<script src="/plugins/platform-include/js/loginCtrl.js"></script>
<script src="/plugins/platform-include/js/blankCtrl.js"></script>
<style type="text/css">


.w400px {
  width: 50%;
  height:400px;
  /*padding-top: 8%;
  padding-left: 20%;
  padding-bottom: 30px; */
  margin:8% auto;
}
.loginTitle {
background:url(images/login-mz/2-1.png) no-repeat;
height: 263px;
width: 771px;
margin:15% auto auto auto;
}

.loginForm {
}
.username {
	background:url(images/login-mz/3.png) no-repeat;
    height: 50px;
    width: 410px;
    padding-left:62px;
    border:none;
    font-size:16px;
}
.password {
	background:url(images/login-mz/4.png) no-repeat;
	height: 50px;
    width: 410px;
    padding-left:62px;
    border:none;
    font-size:16px;
    margin-top:10px;
}
.submit {
	background:url(images/login-mz/5.png) no-repeat;
	height: 50px;
    width: 410px;
    border:none;
    margin-left:8px;
    margin-top:20px;
}

</style>
</head>

<body style="height:1080px;background:url(images/login-mz/bg.jpg) no-repeat">

<div ng-app="app" class="container-fluid" ng-controller="loginCtrl" ng-init="init();">
    <div class="loginBox">
        <div class="col-xs-3"></div>
        <div class="col-xs-6 text-left" >
         <div class="loginTitle"></div>
         <div class="w400px">
            <form class="form-horizontal loginForm" novalidate autocomplete="off">
                <div class="form-group" style="height:22px;">
                	<!-- <label class="col-xs-2"></label> -->
                    <div style="color:red;" ng-bind="errorMessage"></div>
                </div>
                <div class="form-group">
                	<label class="col-xs-2"></label>
                    <div class="input-group col-xs-10">
                    	<input name="username" type="text" style="display:none;">
                        <input name="username" placeholder="用户名" class="username" type="text" autocomplete="off"
                               ng-model="user.username" ng-change="loginCheck();">
                    </div>
                </div>
                <div class="form-group">
                	<label class="col-xs-2"></label>
                    <div class="input-group col-xs-10">
                    	<input name="password" type="password" style="display:none;">
                        <input name="password" placeholder="密码" class="password" type="password" autocomplete="off"
                               ng-model="user.password" ng-change="loginCheck();">
                    </div>
                </div>
                <div class="form-group">
                	<label class="col-xs-2"></label>
                    <div ng-class="col-xs-10">
                        <button  autocomplete="off" id="loginBtn" class="submit" ng-click="loginSubmit();"></button>
                    </div>
                </div>
            </form>
	        </div>
	        </div>
        <div class="col-xs-3"></div>
    </div>
</div>
</body>