.page-container{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.page-container .sidebar{
  position: absolute;
  top: 70px;
  left: 0;
  bottom:0;
  width: 180px;
  background-color: #efefef;
  border-right: 1px solid #d9d9d9;
  overflow: hidden;
  z-index: 1000;
}
/* 左侧菜单样式 */
.tree-nav{
  width: 100%;
  height: 100%;
  overflow: auto;
}
.tree-nav ul{
  padding: 0;
}
.tree-nav a{
  position: relative;
  display: block;
  padding: 0 15px 0 30px;
  color: #666666;
  line-height: 35px;
  white-space: nowrap;
  text-decoration: none;
}
.tree-nav a:hover{
  color: #337ab7 !important;
}
.tree-nav a.active{
  color: #fff !important;
  background-color: #337ab7;
}
.tree-nav a span{
  display: block;
  text-overflow:ellipsis;
  overflow: hidden;
}
.tree-nav .level0 > li > a{
  color: #333;
  font-size: 16px;
  line-height: 40px;
}
.tree-nav .level1{
  display: none;
}
.tree-nav .level1 > li > a{
  padding-left: 45px;
}
.tree-nav .level2{
  display: none;
}
.tree-nav .level2 > li > a{
  padding-left: 55px;
  font-size: 12px;
}
.tree-nav .level3{
  display: none;
}
.tree-nav .level3 > li > a{
  padding-left: 65px;
  font-size: 12px;
}
.tree-nav i{
  position: absolute;
  top: 12px;
  left: 5px;
  display: inline-block;
  width: 20px;
  height: 18px;
  background-image: url('/images/zhuGeNew/icons.png');
  vertical-align: middle;
}
.zs-3037{
  background-position: 0 -36px;
}
.tree-nav a:hover .zs-3037{
  background-position: -20px -36px;
}
.zs-3038{
  background-position: 0 0;
}
.tree-nav a:hover .zs-3038{
  background-position: -20px 0;
}
.zs-3039{
  background-position: 0 -18px;
}
.tree-nav a:hover .zs-3039{
  background-position: -20px -18px;
}
.zs-3040{
  background-position: 0 -54px;
}
.tree-nav a:hover .zs-3040{
  background-position: -20px -54px;
}
.zs-3041{
  background-position: 0 -107px;
}
.tree-nav a:hover .zs-3041{
  background-position: -20px -107px;
}
.zs-304101{
  background-position: 0 -72px;
}
.tree-nav a:hover .zs-304101{
  background-position: -20px -72px;
}
.zs-3042{
  background-position: 0 -90px;
}
.tree-nav a:hover .zs-3042{
  background-position: -20px -90px;
}
.arrow{
  position: absolute;
  top: 50%;
  right: 10px;
  width: 0;
  height: 0;
  margin-top: -3px;
  font-size: 0;
}
.arrow.right{
  border-top: 4px solid transparent;
  border-right: none;
  border-bottom: 4px solid transparent;
  border-left: 4px solid #666;
}
.arrow.bottom{
  border-top: 4px solid #666;
  border-right: 4px solid transparent;
  border-bottom: none;
  border-left: 4px solid transparent;
}
.tree-nav a:hover .arrow.right{
  border-left-color: #337ab7;
}
.tree-nav a:hover .arrow.bottom{
  border-top-color: #337ab7;
}
.tree-nav a.active .arrow.right{
  border-left-color: #fff;
}
.tree-nav a.active .arrow.bottom{
  border-top-color: #fff;
}
/* 左侧菜单样式结束 */
.page-container .content{
  position: absolute;
  top: 70px;
  right: 0;
  bottom: 30px;
  left: 180px;
}
.page-container .content iframe{
  width: 100%;
  height: 100%;
  border: none;
}
.ifcontent{
  padding: 0 16px;
  overflow: auto;
}
.ifcontent .container-fluid{
  position: absolute;
  top: 53px;
  right: 16px;
  bottom: 32px;
  left: 16px;
  border-radius: 4px;
  background-color: #fff;
  padding-top: 50px;
  overflow: auto;
}
.ifcontent .form-group {
    margin-bottom: 18px;
}
.form-control[disabled],
.form-control[readonly],
fieldset[disabled] .form-control {
