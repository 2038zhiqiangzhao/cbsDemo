.close-tab {
    font-size: 12px;
    position:absolute;
    right: 5px;
    top: 5px;
    padding-left: 25px;
    z-index: 100;
    cursor: hand;
    cursor: pointer;
    color: #dfe4e6;
}
.close-tab:hover{
	font-size: 12px;
    position:absolute;
    right: 5px;
    top: 5px;
    padding-left: 25px;
    z-index: 100;
    cursor: hand;
    cursor: pointer;
    color: #de8e8e;
}

.nav-tabs {position: relative;}
.nav-tabs li{position: relative;}

.iframeClass {
    z-index: 1;
    visibility: inherit;
    width: 100%;
    frameborder: 'no';
    border: 0;
    marginwidth: 0;
    marginheight: 0;
    scrolling-x: 'no';
    scrolling-y: auto;
    allowtransparency: true;
}

.tab-drop {
    margin-right: 30px;
}

.rightMenu {
    position: absolute;
    width: 150px;
    z-index: 99;
    display: none;
}

.dragBack {
    background-color: #ddd;
}
