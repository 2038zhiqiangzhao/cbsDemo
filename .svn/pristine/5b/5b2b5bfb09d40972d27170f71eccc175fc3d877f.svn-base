/* 创建活动 */
.nav-steps{
  position: relative;
  width: 740px;
  height: 53px;
  border-bottom: 3px solid #e4e7f0;
  margin-left: 26px;
  margin-bottom: 72px;
}
.nav-steps li{
  position: absolute;
  top: 0;
  font-size: 16px;
  cursor: pointer;
  z-index: 2;
}
.nav-steps li .circle{
  width: 44px;
  height: 44px;
  color: #B6BBBF;
  font-size: 20px;
  text-align: center;
  line-height: 44px;
  border-radius: 22px;
  background-color: #e4e7f0;
  margin:8px auto 0;
}
.nav-steps li .line{
  position: absolute;
  top:50px;
  height: 3px;
  width: 200px;
  z-index: -1;
}
.nav-steps li.step1{
  left: -33px;
}
.nav-steps li.step1 .line{
  left: 33px;
  width: 185px;
}
.nav-steps li.step2{
  left: 340px;
}
.nav-steps li.step2 .line{
  left: -155px;
  width: 370px;
}
.nav-steps li.step3{
  right: -33px;
}
.nav-steps li.step3 .line{
  right: 33px;
  width: 185px;
}
.nav-steps li.active div{
  color: #fff;
  background-color:#2bca4c;
}
.steps-content,
.steps2-content{
  display: none;
}
.steps-content.active,
.steps2-content.active{
  display: block;
}
.nav-steps2{
  margin-bottom: 37px;
  margin-left: -7px;
}
.nav-steps2 li{
  display: inline-block;
  color: #91A1AE;
  font-size: 13px;
  margin-right:70px;
  cursor: pointer;
}
.nav-steps2 li.active{
  color: #3697FF;
}
.nav-steps2 .active:after{
  content: "";
  display: block;
  width: 0;
  border-top:6px solid #3697FF;
  border-left:6px solid transparent;
  border-right:6px solid transparent;
  margin: 10px auto 0;
}
@media (max-width: 992px) {
