body{
  font-family: arial,"Microsoft YaHei",tahoma,'Hiragino Sans GB',sans-serif;
  background-color: #fff;
}
.page-container{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.page-container .sidebar{
  position: absolute;
  top: 70px;
  left: 0;
  bottom:0;
  width: 180px;
  background-color: #efefef;
  border-right: 1px solid #d9d9d9;
  overflow: hidden;
  z-index: 1000;
}
.page-container .sidebar-nav li{
  color:#BABBBD;
  line-height: 44px;
  font-size: 12px;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  cursor: pointer;
}
.page-container .sidebar-nav .cls{
  position: relative;
  background-color: #3e474f;
}
.page-container .sidebar-nav .cls:after{
  position: absolute;
  top: 0;
  left: 0;
  display: block;
  content: "";
  width: 0;
  height: 100%;
  border-left: 5px solid #3399FE;
  border-top: 5px solid transparent;
  border-bottom: 5px solid transparent;
  padding-left: 10px;
}
.page-container .sidebar-nav li.active {
  background-color: #2A3642;
}
.page-container .sidebar-nav a{
  display: block;
  color:#BABBBD;
  text-decoration: none;
  padding:0 18px;
}
.page-container .sidebar-nav a:hover{
  color: #fff;
}
.page-container .sidebar-nav i{
  font-size: 14px;
  margin-right: 5px;
}
.page-container .sidebar-nav > li > ul{
  display: none;
}
.page-container .content{
  position: absolute;
  top: 70px;
  right: 0;
  bottom: 30px;
  left: 180px;
}
.page-container .content iframe{
  width: 100%;
  height: 100%;
  border: none;
}
.ifcontent{
  color: #546776;
  padding: 0 16px;
  overflow: auto;
}
.ifcontent .container-fluid{
  position: absolute;
  top: 53px;
  right: 16px;
  bottom: 32px;
  left: 16px;
  border-radius: 4px;
  background-color: #fff;
  padding-top: 50px;
  overflow: auto;
}
.ifcontent .form-group {
    margin-bottom: 18px;
}
.form-control[disabled],
.form-control[readonly],
fieldset[disabled] .form-control {
  background-color: #fafafa;
  cursor: default;
}
input[type="radio"][disabled],
input[type="checkbox"][disabled],
input[type="radio"].disabled,
input[type="checkbox"].disabled,
fieldset[disabled] input[type="radio"],
fieldset[disabled] input[type="checkbox"] {
  cursor: default;
}
.ifcontent .page-header{
  color:#36434C;
  font-size: 18px;
  border:none;
  margin:0;
  padding: 18px 0 16px 19px;
}
.ifcontent .page-header .glyphicon{
  color:#989898;
  font-size: 14px;
  margin-left: 10px;
}
@media (max-width: 768px) {
  .page-container .content{
    top: 101px;
    left: 0;
    bottom: 16px;
  }
  .page-container .sidebar{
    display: none;
    top:0;
    z-index: 2000;
  }
  .ifcontent .page-header{
    padding-left: 0;
  }
  .ifcontent .container-fluid{
    right: 0;
    left: 0;
  }
}
