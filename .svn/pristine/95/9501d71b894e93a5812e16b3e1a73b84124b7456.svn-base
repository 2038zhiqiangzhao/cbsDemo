/*timepicker*/
[ng-cloak],.ng-cloak{
    display: none;
}
.prev-btns,.next-btns{
    display: -webkit-box;
}
.prev-btns.title{
    margin-right: 56px;
}
.prev-btns.title>* {
    background: none;
    opacity: 1;
}
.prev-btns .xdsoft_prev{
    background-position: -45px 0!important;
    width: 18px!important;
    margin: 0!important;
}
.next-btns .xdsoft_next {
    background-position: -45px -15px !important;
    width: 18px !important;
    margin: 0 !important;
}
.xdsoft_datetimepicker .xdsoft_datepicker.active+.xdsoft_timepicker{
    margin-top:0;
    margin-bottom:0;
}
.timeiconbg{
    border: 1px #ddd solid;
    padding: 0 10px;
    background: url("/plugins/common/images/time-input-icon.png") 95% no-repeat;
}
.inputSubmit{
    padding: 0 10px;
    border: 1px #ddd solid;
}

/*类目选择效果*/
.categorySelete-box{
    margin: 0;
    padding:0;
    width: 100%;
    list-style:none;
    max-height: 160px;
    overflow-x: hidden;
    overflow-y: auto;
}
.categorySelete-box>li{
    width:100%;
    list-style: none;
    height: 32px;
    line-height: 32px;
    position: relative;
    overflow: hidden;
    background: #ffffff;
    border-bottom:1px solid #dee5e7;
}
.categorySelete-box>li>a{
    display: inline-block;
    padding:0 15px;
    cursor: pointer;
    width:100%;
}
.categorySelete-box>li>i{
    position:absolute;
    right: 15px;
    top: 7px;
    cursor: pointer;
}
.categorySelete-box>li>.active{
    background: #fafbfc;
}
.categorySelete-box>li>a:hover{
    background: #fafbfc;
}
