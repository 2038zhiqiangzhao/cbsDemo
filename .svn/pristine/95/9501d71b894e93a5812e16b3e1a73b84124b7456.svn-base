/*修复ztree只有最后一个显示二级目录的问题*/
.line{
    height: auto!important;
}
/*修复侧边栏二级目录分割不显示问题*/
.nav .line{
    height:2px !important;
}

.hoverPoint:hover{
    cursor: pointer;
}

.nowrap{
    max-width:100%;
    overflow:hidden;
    white-space:nowrap;
    text-overflow:ellipsis
}
.nowrap-multi-2{
    display:-webkit-box;
    overflow:hidden;
    text-overflow:ellipsis;
    -webkit-box-orient:vertical;
    -webkit-line-clamp:2
}
.nowrap-multi-3{
    display:-webkit-box;
    overflow:hidden;
    text-overflow:ellipsis;
    -webkit-box-orient:vertical;
    -webkit-line-clamp:3
}


/*添加参与商家*/
.siteSpan{
    display: inline-block;
    padding: 6px;
    border-radius: 4px;
    border: 1px #ddd solid;
    margin-top: 5px;
    margin-left: 5px;
}
.siteSpan:nth-child(1){
    margin-left: 0;
}
.siteSpan:hover{
    cursor: pointer;
}
.siteSpan i{
    color: #ddd;
}
.provinces{
    display: inline-block;
    height: 30px;
    line-height: 30px;
    float: left;
    padding: 0 10px;
    position: relative;
}
.provinces:hover{
    cursor: pointer;
}
.provinces:nth-child(1){
    padding: 0;
}
.provinces_city{
    position: absolute;
    left: 0;
    top: 40px;
    width: 400px;
    border: 1px #ddd solid;
    border-radius: 4px;
    padding: 20px;
    background: #F6F8F8;

}
.provinces_city .siteSpan{
    padding-top:0;
    padding-bottom: 0;
    background: #fff;
}
.provinces_city .siteSpan:nth-child(1){
    margin-left: 5px;
}

