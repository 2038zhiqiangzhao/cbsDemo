/*类目选择效果*/
.categorySelete-box{
  margin: 0;
  padding:0;
  width: 100%;
  list-style:none;
  max-height: 160px;
  overflow-x: hidden;
  overflow-y: auto;
}
.categorySelete-box>li{
  width:100%;
  list-style: none;
  height: 32px;
  line-height: 32px;
  position: relative;
  overflow: hidden;
  background: #ffffff;
  border-bottom:1px solid #dee5e7;
}
.categorySelete-box>li>a{
  display: inline-block;
  padding:0 15px;
  cursor: pointer;
  width:100%;
}
.categorySelete-box>li>i{
  position:absolute;
  right: 15px;
  top: 7px;
  cursor: pointer;
}
.categorySelete-box>li>.active{
  background: #fafbfc;
}
.categorySelete-box>li>a:hover{
  background: #fafbfc;
}
